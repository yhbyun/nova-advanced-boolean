{
    "name": "yhbyun/nova-advanced-boolean",
    "description": "An alternative to the Boolean component in Laravel Nova",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0"
    },
    "autoload": {
        "psr-4": {
            "Yhbyun\\NovaAdvancedBoolean\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Yhbyun\\NovaAdvancedBoolean\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
